    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/TVUBMTool.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console":"integratedTerminal"
        },
        {
            "name": ".NET Core Launch (remote console)",
            "type": "coreclr",
            "request": "launch",
            "program": "~/publish/TVUBMTool.dll",
            "args": [],
            "cwd": "~/publish",
            "stopAtEntry": true,
            "justMyCode": false,
            "console": "externalTerminal",
            "pipeTransport": {
                "pipeCwd": "${workspaceFolder}",
                "pipeProgram": "C:\\Windows\\System32\\OpenSSH\\ssh.exe",
                "pipeArgs": ["root@10.12.32.91"],
                "debuggerPath": "~/vsdbg/vsdbg"
            }
        }
        
        
    ]
}